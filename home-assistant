                        const now = new Date();
                        const diffInMs = now - lastChanged;

                        const diffInHours = Math.floor(diffInMs / (1000 * 60 * 60));
                        const diffInMinutes = Math.floor((diffInMs % (1000 * 60 * 60)) / (1000 * 60));

