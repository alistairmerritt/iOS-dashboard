              num: |
                [[[ 
                switch (entity.state) {
                  case 'open':
                    return 'Open';
                  case 'closed':
                    return 'Closed';
                  case 'opening':
                    return 'Opening';
                  case 'closing':
                    return 'Closing';
                  default:
                    return 'Unknown State'; // Optional: Handle unexpected states
                }
                ]]]
